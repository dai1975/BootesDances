<?xml version="1.0" encoding="UTF-8" ?>
<GUILayout>
<Window Type="app/ToggleImageButton" Name="LineButton">
	<Property Name="UnifiedPosition" Value="{{0,0},{0,0}}" />
	<Property Name="UnifiedSize" Value="{{0,32},{0,32}}" />
	<Property Name="NormalImage"      Value="set:PenButton image:LineNormal" />
	<Property Name="HoverImage"       Value="set:PenButton image:LineHover"  />
	<Property Name="PushedImage"      Value="set:PenButton image:LinePushed" />
	<Property Name="PushedHoverImage" Value="set:PenButton image:LinePushedHover" />
	<Property Name="DisabledImage"    Value="set:PenButton image:LineDisabled" />
</Window>

</GUILayout>
